{
  "name": "web",
  "version": "1.11.2",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*": [
      "secretlint"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "pnpm db:generate && next dev",
    "build": "pnpm db:generate && next build",
    "sitemap": "next-sitemap",
    "start": "next start",
    "lint": "eslint .",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build --webpack",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "dev:analyze": "cross-env ANALYZE=true next dev",
    "dup": "jscpd src",
    "secretlint": "secretlint \"**/*\"",
    "secretlint:check": "secretlint \"**/*\"",
    "typecheck:coverage": "type-coverage --threshold 90",
    "knip": "knip --no-exit-code",
    "spellcheck": "cspell \"**/*\" --no-progress --show-suggestions",
    "gen:client": "openapi --input http://localhost:3000/api/openapi --output ./src/generated/client --client axios",
    "doc:gen": "typedoc",
    "db:generate": "zenstack generate --schema prisma/schema.zmodel",
    "db:migrate": "pnpm db:generate && prisma migrate dev",
    "db:migrate-prod": "pnpm db:generate && prisma migrate deploy",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "test:stryker": "pnpm stryker run",
    "test": "cross-env TZ=UTC vitest run --config vitest.config.ts --no-file-parallelism",
    "test:int": "cross-env TZ=UTC vitest run src/tests/integration --config vitest.config.ts --no-file-parallelism",
    "test:unit": "cross-env TZ=UTC vitest run src/tests/unit --config vitest.config.ts",
    "test:e2e": "pnpm exec playwright test",
    "test:coverage": "cross-env TZ=UTC vitest run --coverage --config vitest.config.ts --no-file-parallelism",
    "changeset": "changeset",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "release": "changeset publish",
    "trigger": "pnpm dlx trigger.dev@latest dev"
  },
